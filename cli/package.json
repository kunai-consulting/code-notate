{
  "name": "@kunai-consulting/code-notate",
  "description": "CLI tools for automatically documenting your code",
  "version": "0.0.0",
  "author": "Kunai Consulting",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "pnpm build && ./bin/dev.js gen-api",
    "gen-api": "pnpm build && ./bin/dev.js gen-api",
    "gen-docs": "pnpm build && ./bin/dev.js gen-docs",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "run": "pnpm build && ./bin/run.js gen-api",
    "test": "vitest --no-watch",
    "test-watch": "vitest",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "bin": {
    "cn": "./bin/run.js"
  },
  "bugs": "https://github.com/kunai-consulting/code-notate/issues",
  "dependencies": {
    "@anthropic-ai/sdk": "0.37.0",
    "@kunai-consulting/code-notate-core": "file:../core",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/node": "^18",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5",
    "vitest": "^3.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/kunai-consulting/code-notate",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "cn",
    "dirname": "cn",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "kunai-consulting/code-notate"
}
